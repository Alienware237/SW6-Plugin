{
    "name": "pok/multiple-item-to-shopping-cart",
    "description": "Plugin to add multiple products to the shopping cart",
    "type": "shopware-platform-plugin",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Piam",
	    "email": "kevinpiam3@yahoo.com"
        }
    ],
    "require": {
        "shopware/core": "~6.6.0"
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix"
    },
    "extra": {
        "shopware-plugin-class": "PokMultipleItemToShoppingCart\\PokMultipleItemToShoppingCart",
        "label": {
            "de-DE": "OKPT plugin",
            "en-GB": "OKPT plugin"
        }
    },
    "autoload": {
        "psr-4": {
            "PokMultipleItemToShoppingCart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PokMultipleItemToShoppingCart\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.63"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
