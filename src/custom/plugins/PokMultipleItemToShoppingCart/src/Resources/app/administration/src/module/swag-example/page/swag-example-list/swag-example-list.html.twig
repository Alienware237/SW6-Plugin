{# <plugin root>/src/Resources/app/administration/src/module/swag-example/page/swag-example-list/swag-example-list.html.twig #}

<template>
    <sw-page>
        <template #content>
            <sw-card>
                <sw-data-grid :dataSource="logs" :columns="columns">
                    <template #column-customer_id="{ item }">
                        {{ item.customer_id }}
                    </template>
                    <template #column-product_id="{ item }">
                        {{ item.product_id }}
                    </template>
                    <template #column-quantity="{ item }">
                        {{ item.quantity }}
                    </template>
                    <template #column-created_at="{ item }">
                        {{ item.created_at }}
                    </template>
                    <template #actions="{ item }">
                        <sw-button variant="primary" @click="viewDetails(item)">View Details</sw-button>
                    </template>
                </sw-data-grid>
            </sw-card>
        </template>
    </sw-page>
</template>

